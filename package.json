{
  "name": "nuxt-seo-kit-mono",
  "private": true,
  "packageManager": "pnpm@8.7.4",
  "description": "The all-in-one SEO module for Nuxt 3.",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-seo-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-seo-kit.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-seo-kit/issues"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "nuxi dev .playground",
    "lint": "eslint . --fix",
    "release": "bumpp layer/package.json module/package.json --commit --push --tag",
    "test": "nuxi prepare .playground && npx vue-tsc --noEmit && pnpm lint && vitest",
    "typecheck": "tsc --noEmit --strict --typeCheck"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.1",
    "@nuxt/test-utils": "3.7.1",
    "@nuxtjs/i18n": "8.0.0-rc.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "execa": "^8.0.1",
    "nitropack": "^2.6.2",
    "nuxt": "^3.7.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}
